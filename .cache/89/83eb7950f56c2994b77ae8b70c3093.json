sicMaterial({ color: 0x00ff00 });\nconst torus = new THREE.Mesh(geometry, material);\nconst torus2 = new THREE.Mesh(geometry, material);\nconst torus3 = new THREE.Mesh(geometry, material);\nscene.add(torus);\nscene.add(torus2);\nscene.add(torus3);\n\ncamera.position.z = 60;\ntorus2.position.x = -20;\ntorus3.position.x = 20;\n\nconst toruss = [torus, torus2, torus3];\n\n\n\n// gsap.fromTo(\n//   torus.position,\n//   {x: -40, y: -40}, \n//   { y: 40, duration: 1 }\n// )\n// gsap.fromTo(\n//   torus.position,\n//   {x: -40, y: 40}, \n//   { x: 40, duration: 1, delay: 1 }\n// )\n// gsap.fromTo(\n//   torus.position,\n//   {x: 40, y: 40}, \n//   { y: -40, duration: 1, delay: 2 }\n// )\n// gsap.fromTo(\n//   torus.position,\n//   {x: 40, y: -40}, \n//   { x: -40, duration: 1, delay: 3 }\n// )\n\n// const tl = gsap.timeline({repeat: -1});\nconst controls = new OrbitControls(camera, renderer.domElement);\nconst stats = Stats();\ndocument.body.appendChild(stats.dom);\n\nconst animate = () => {\n  requestAnimationFrame(animate);\n\n  // torus.rotation.x += 0.1;\n  // torus.rotation.y += 0.1;\n  // torus.rotation.z += 0.01;\n\n  // tl.to(torus.position, {z: 0, duration: 0.4 })\n  // tl.to(torus.position, {y: 40, duration: 0.4 })\n  // tl.to(torus.position, {x: -40, duration: 0.4 })\n  // tl.to(torus.position, {z: -40, duration: 0.4 })\n  // tl.to(torus.position, {y: -40, duration: 0.4 })\n  // tl.to(torus.position, {x: 40, duration: 0.4 })\n\n  toruss.forEach((torus, index) => {\n\n    gsap.to(torus.position, {\n      x: 0,\n      stagger: function(){\n        return index * 0.5\n      }\n    })\n  });\n\n  controls.update();\n  stats.update();\n  renderer.render(scene, camera);\n\n};\n\nanimate();\n\n//change color of the torus\n"},"lineCount":null}},"error":null,"hash":"f553510bfae99a0efc37ca1d5bc3f975","cacheData":{"env":{}}}